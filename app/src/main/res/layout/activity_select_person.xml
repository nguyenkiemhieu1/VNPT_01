                            android:layout_height="wrap_content" />
                        <TextView
                            android:id="@+id/tv_dong_xuly_lt"
                            android:textStyle="bold"
                            android:text="@string/tv_PH"
                            android:layout_width="wrap_content"
                            android:fontFamily="@font/arial"
                            android:gravity="center"
