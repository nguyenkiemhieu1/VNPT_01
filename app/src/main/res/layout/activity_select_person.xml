                            android:layout_weight="5"
                            android:paddingLeft="@dimen/_5dp"
                            android:layout_marginRight="@dimen/_20dp"
                            android:textColor="@color/md_white"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:textStyle="bold"
                            android:text="@string/tv_xuly_chinh"
                            android:fontFamily="@font/arial"
                            android:layout_width="wrap_content"
                            android:layout_marginRight="@dimen/_20dp"
                            android:textColor="@color/md_white"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:textStyle="bold"
                            android:text="@string/tv_dong_xuly"
                            android:fontFamily="@font/arial"
                            android:layout_width="wrap_content"
                            android:layout_marginRight="@dimen/_5dp"
                            android:textColor="@color/md_white"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:id="@+id/layout_person_process"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginBottom="@dimen/_10dp"
                            android:background="#fff">
                        </LinearLayout>
                    </ScrollView>
                </LinearLayout>
                <LinearLayout
                    android:visibility="gone"
                    android:background="@color/md_grey_200"
                    android:id="@+id/layout_send_notify"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_3dp"
                    android:layout_marginRight="@dimen/_3dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40dp"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:background="@drawable/background_border_top_green">
                        <TextView
                            android:id="@+id/tv_hoten_donvi"
                            android:textStyle="bold"
                            android:fontFamily="@font/arial"
                            android:text="@string/tv_ho_ten_dv"
                            android:layout_width="wrap_content"
                            android:paddingLeft="@dimen/_5dp"
                            android:textColor="@color/md_white"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:id="@+id/tv_xuly_chinh_donvi"
                            android:textStyle="bold"
                            android:fontFamily="@font/arial"
                            android:text="@string/tv_xuly_chinh"
                            android:paddingRight="@dimen/_5dp"
                            android:layout_width="match_parent"
                            android:textColor="@color/md_white"
                            android:layout_height="wrap_content"
                            android:gravity="right"/>
                    </LinearLayout>
                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:id="@+id/layout_donvi"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginBottom="@dimen/_10dp"
                            android:background="#fff">
                    </LinearLayout>
                    </ScrollView>
                </LinearLayout>
                <LinearLayout
                    android:visibility="gone"
                    android:background="@color/md_grey_200"
                    android:id="@+id/layout_send_xemdb"
