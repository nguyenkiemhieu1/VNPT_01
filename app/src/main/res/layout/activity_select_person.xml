                    </LinearLayout>
                    <TextView
                        android:id="@+id/btnMorongLT"
                        android:text="@string/tv_nhan_chon"
                        android:fontFamily="@font/arial"
                        android:textStyle="bold"
                        android:drawableLeft="@drawable/ic_down"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_marginRight="@dimen/_20dp"
                        android:textColor="@color/md_grey_800"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/btnThuGonLT"
                        android:drawableLeft="@drawable/ic_up"
                        android:text="@string/tv_thugon"
                        android:fontFamily="@font/arial"
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:layout_marginRight="@dimen/_20dp"
                        android:layout_gravity="center"
                        android:textColor="@color/md_grey_800"
                        android:layout_height="wrap_content" />
                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:id="@+id/layout_donvi_lt"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginBottom="@dimen/_10dp"
                            android:background="#fff">
                        </LinearLayout>
                    </ScrollView>
                </LinearLayout>
            </LinearLayout>
    </LinearLayout>
</LinearLayout>
